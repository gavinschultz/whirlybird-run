            <kind>1</kind>
            <string>if (global.frame_count % 3) == 0
{
    score++;
}
heli_hspeed = 4;
heli_vspeed = 4;

if (global.frame_count % fuel_rate == 0 &amp;&amp; fuel &gt; 0)
{
    fuel--;
}

if (fuel == 0)
{
    y += 2;
}
else
{
    if (keyboard_check(vk_up))
    {
        y -= heli_vspeed;
    }
    if (keyboard_check(vk_down))
    {
        y += heli_vspeed;
    }
}
if (keyboard_check(vk_left))
{
    x -= heli_hspeed;
}
if (keyboard_check(vk_right))
{
    x += heli_hspeed;
}

if (x &lt; view_xview[0] + 64)
    x = view_xview[0] + 64;
if (x &gt; view_xview[0] + view_wview[0] - 64)
    x = view_xview[0] + view_wview[0] - 64;
if (y &lt; 90)
    y = 90;
    
//pitch_change = 1 + (1-x/384);
if (fuel &lt;= 0)
{
    audio_sound_pitch(sound_heli1, 0.6);
}
else
{
    pitch_range_lower = 0.9;
    pitch_range_upper = 1.4;
    var altitude_sound_pitch = pitch_range_lower + (700-y)/(620/(pitch_range_upper - pitch_range_lower));
    audio_sound_pitch(sound_heli1, altitude_sound_pitch);
    /*
    if (y &gt; 500 &amp;&amp; audio_sound_get_pitch(sound_heli1) &gt;= 1)
        audio_sound_pitch(sound_heli1, 0.8);
    else if (y &lt; 250 &amp;&amp; audio_sound_get_pitch(sound_heli1) &lt;= 1)
        audio_sound_pitch(sound_heli1, 1.15);
    else
        audio_sound_pitch(sound_heli1, 1);
    */
}
    
if (bomb_cooldown &gt; 0) 
{
    bomb_cooldown -= 1;
}
if (keyboard_check(vk_space))
{
    if (fire_cooldown &gt; 0)
    {
        fire_cooldown -= 1;
    }
    else
    {
        FireBullet();
        fire_cooldown = initial_fire_cooldown;
    }
    
    if (bomb_cooldown == 0)
    {
        DropBomb();
        bomb_cooldown = initial_bomb_cooldown;
    }
}
else
{
    fire_cooldown = 0;
}
</string>
